@model Usuario

@{
    ViewData["Title"] = "Detalles Usuario";
}

<h1>@ViewData["Title"]</h1>
<p>En esta página se muestran los detalles de los Usuarios.</p>

<form>
    <div class="mb-3">
        <label class="form-label">Nombre</label>
        <input asp-for="nombre" class="form-control " readonly />
    </div>
    <div class="mb-3">
        <label class="form-label">Apellido</label>
        <input asp-for="apellido" class="form-control " readonly />
    </div>
    <div class="mb-3">
        <label class="form-label">Número de Seguridad</label>
        <input asp-for="numeroSeguridad" class="form-control " readonly />
    </div>
    <div class="row">
        <div class="col-6">
            <button type="button" class="btn btn-success" onclick="location.href='@Url.Action("Index","Usuario")'">Volver</button>
        </div>
    </div>
</form>



@model Usuario

@{
    ViewData["Title"] = "Creación de Usuarios";
}

<h1>@ViewData["Title"]</h1>
<p>En esta página se crean los usuarios.</p>

<form method="post">
    <div class="mb-3">
        <label asp-for="cedula" class="form-label">Cedula</label>
        <input asp-for="cedula" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="nombre" class="form-label">Nombre</label>
        <input asp-for="nombre" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="apellido" class="form-label">Apellido</label>
        <input asp-for="apellido" class="form-control" />
    </div
    <div class="mb-3">
        <label asp-for="login" class="form-label">Usuario</label>
        <input asp-for="login" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="contrasenia" class="form-label">Contraseña</label>
        <input asp-for="contrasenia" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="numeroSeguridad" class="form-label">NumeroSeguridad</label>
        <input asp-for="numeroSeguridad" class="form-control" />
    </div>
       <div class="row">
        <div class="col-6">
            <button type="submit" class="btn btn-primary">Guardar</button>
        </div>
        <div class="col-6">
            <a asp-action="Cancelar" class="btn btn-secondary">Cancelar</a>
        </div>
    </div>
</form>

